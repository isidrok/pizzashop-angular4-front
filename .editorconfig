# http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
end_of_line = lf
max_line_length = 80
trim_trailing_whitespace = false
insert_final_newline = true

[*html]
max_line_length = 0
insert_final_newline = false

[*.md]
max_line_length = 0
trim_trailing_whitespace = false
